{
    "name"        : "scr-be/magick-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://wonka-bundle.oss.scr.be/home",
    "description" : "Simple abstraction layer over PHP's IMagick library.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://wonka-bundle.oss.scr.be/issues",
        "source" : "https://wonka-bundle.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "magick-bundle", "scribe-magick-bundle", "magick", "imagick", "imagemagick", "symfony", "scribe", "scribenet"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require": {
        "php"                          : ">=5.6.4,~5.6|~7.0",
        "ext-imagick"                  : ">=3.3.0",
        "scr-be/wonka-bundle"          : "~0.2"
    },
    "require-dev" : {
        "symfony/symfony"             : "~2.8|~3.0",
        "symfony/monolog-bundle"      : "~2.8|~3.0",
        "symfony/stopwatch"           : "~2.8|~3.0",
        "symfony/var-dumper"          : "~2.8|~3.0",
        "symfony/debug-bundle"        : "~2.8|~3.0",
        "symfony/phpunit-bridge"      : "~2.8|~3.0",
        "phpunit/phpunit"             : "~5.0",
        "codacy/coverage"             : "~1.0",
        "satooshi/php-coveralls"      : "~0.7",
        "chriskite/phactory"          : "~0.4"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.1"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\MagickBundle\\" : "src/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
