{
    "name"        : "scr-be/teavee-image-magic-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://scr.be/arthur-doctrine-fixtures-bundle",
    "description" : "A simple abstraction image handlers, currently using the IMagick extension.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://scr.be/arthur-doctrine-fixtures-bundle/issues",
        "source" : "https://scr.be/arthur-doctrine-fixtures-bundle/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "teavee-image-magic-bundle", "scribe-image-magic-bundle", "magick", "imagick", "imagemagick", "symfony", "scribe"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require": {
        "php"                          : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-bundle"          : "~0.3"
    },
    "require-dev" : {
        "symfony/symfony"             : "~2.8|~3.0",
        "symfony/monolog-bundle"      : "~2.8|~3.0",
        "symfony/stopwatch"           : "~2.8|~3.0",
        "symfony/var-dumper"          : "~2.8|~3.0",
        "symfony/debug-bundle"        : "~2.8|~3.0",
        "symfony/phpunit-bridge"      : "~2.8|~3.0",
        "phpunit/phpunit"             : "~5.0",
        "codacy/coverage"             : "~1.0",
        "satooshi/php-coveralls"      : "~1.0"
    },
    "suggest" : {
        "ext-imagick" : ">=3.3.0"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.4"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Teavee\\ImageMagicBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Teavee\\ImageMagicBundle\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
