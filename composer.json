{
    "name"        : "scribe/magick-bundle",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://wonka-bundle.oss.scr.be/home",
    "description" : "Simple abstraction layer over PHP's IMagick library.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://wonka-bundle.oss.scr.be/issues",
        "source" : "https://wonka-bundle.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "magick-bundle", "scribe-magick-bundle", "magick", "imagick", "imagemagick", "symfony", "scribe", "scribenet"
    ],
    "authors"     : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require": {
        "php"                          : ">=5.6.4,~5.6",
      "ext-imagick"                    : ">=3.3.0RC1",
        "scribe/wonka-library"         : "~0.1@dev",
        "scribe/wonka-bundle"          : "~0.1@dev"
    },
    "require-dev" : {
        "symfony/symfony"             : "~2.7|~2.8@dev",
        "symfony/monolog-bundle"      : "~2.7|~2.8@dev",
        "symfony/stopwatch"           : "~2.7|~2.8@dev",
        "symfony/var-dumper"          : "~2.7|~2.8@dev",
        "symfony/debug-bundle"        : "~2.7|~2.8@dev",
        "symfony/phpunit-bridge"      : "~2.7|~2.8@dev",
        "phpunit/phpunit"             : "~4.6",
        "sensiolabs/security-checker" : "~3.0",
        "scrutinizer/ocular"          : "~1.1",
        "satooshi/php-coveralls"      : "~0.6",
        "chriskite/phactory"          : "~0.4",
        "sami/sami"                   : "~3.0",
        "nikic/php-parser"            : ">=1.0,<1.2"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.1"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\MagickBundle\\" : "src/"
        }
    },
    "config" : {
        "bin-dir"           : "bin",
        "preferred-install" : "auto",
        "minimum-stability" : "dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
